        $(function () {

            $('#inputMethod').change(function () {
                var ddl = $(this).val();
                inputFunction(ddl);
            })

            $('#selectState').change(function () {
                var ddlval = $(this).val();
                var target = $('#selectTownship');
                target.empty();
                if (ddlval != '-1') {
                    var cities = listOfTownship[ddlval];
                    var optionResult = $.map(cities, function (val) {
                        return '<option value="' + val + '">' + val + '</option>'
                    }).join(' ');
                    target.html(optionResult);
                    target.focus();
                }
            })

            $('#back').click(function () {
                window.history.back();
            })


            var forms = document.getElementsByClassName('needs-validation');
            // Loop over them and prevent submission
            var validation = Array.prototype.filter.call(forms, function (form) {
                form.addEventListener('submit', function (event) {

                    if (form.checkValidity() === false) {
                        event.preventDefault();
                        event.stopPropagation();

                    } else {

                        // we can perfom db inserting or ajax 
                    }
                    form.classList.add('was-validated');

                }, false);
            });


            inputFunction($('#inputMethod').val());


        })

        function inputFunction(input) {
            let ddl = input;
            let divEnter = $('#enterAdd');
            let divUpload = $('#uploadAdd');
            let txtArea = $('#txtAreAddress');
            let photo = $('#uploadAddress');

            if (ddl == "enter") {
                txtArea.attr("disabled", false).focus();
                divEnter.removeClass('d-none');
                photo.attr("disabled", true);
                divUpload.addClass('d-none');
            }
            if (ddl == "upload") {
                divEnter.addClass('d-none');
                txtArea.attr("disabled", true);
                divUpload.removeClass('d-none');
                photo.attr("disabled", false).focus();

            }
        }

    </script>
